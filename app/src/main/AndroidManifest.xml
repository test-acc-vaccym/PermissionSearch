<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sakaitakao.android.permissionsearch"
    android:versionCode="4"
    android:versionName="@string/app_version">

    <uses-sdk android:minSdkVersion="7" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" android:description="@string/app_description">
        <activity android:name=".activity.EasySearchActivity" android:label="@string/title_easy_search">
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/title_permission_list"
            android:name=".activity.PermissionListActivity" >
        </activity>
        <activity
            android:label="@string/title_app_list"
            android:name=".activity.ApplicationListActivity" >
        </activity>
        <activity android:name=".activity.AdvancedSearchActivity" android:label="@string/title_advanced_search"></activity>
        <activity android:name=".activity.MainPreferenceActivity" android:label="@string/title_preference"></activity>
        <activity android:name=".activity.SavedAdvancedSearchConditionPreferenceActivity" android:label="@string/title_preference_saved_advanded_search_cond"></activity>
        <activity android:label="@string/title_about" android:name=".activity.AboutActivity"></activity>
        
        </application>

</manifest>